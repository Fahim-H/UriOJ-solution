#include <iostream>
#include<iomanip>
using namespace std;
int main()
{
    int arr[100];
    int highest,index=0;
    for(int i=0;i<100;i++){
        cin>>arr[i];
        highest=arr[0];
    }
    for(int i=0;i<100;i++){
        if(highest<arr[i]){
            highest=arr[i];
            index=i;
        }
    }
    cout<<highest<<" "<<index+1<<endl;
    return 0;
}
 
