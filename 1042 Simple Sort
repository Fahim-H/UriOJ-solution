//#include <bits/stdc++.h>
#include<iostream>
#include<vector>
#include<algorithm>
using namespace std;
 
int main(){
    vector <int> v;
    int num;
    for(int i=1;i<=3;i++){
        cin>>num;
        v.push_back(num);
    }
    vector <int> var=v;
    sort(var.begin(),var.end());
    for(auto x:var){
        cout<<x<<endl;
    }
    cout<<endl;
    for(auto x:v)
        cout<<x<<endl;
 
}
 
