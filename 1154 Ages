#include<iostream>
#include<iomanip>
using namespace std;
int main(){
    int num,sum=0;
    double count=0;
    while(true){
        cin>>num;
        if(num<0)
            break;
 
        sum=sum+num;
        count++;
 
    }
    double avg=sum/count;
    cout<<fixed<<setprecision(2)<<avg<<endl;
    return 0;
}
 
