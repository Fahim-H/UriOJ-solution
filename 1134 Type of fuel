#include<iostream>
#include<iomanip>
using namespace std;
int main(){
    int num,alcohol=0,gasolina=0,dissel=0;
    while(true){
        cin>>num;
        if(num==4)
            break;
        else if(num==1)
            alcohol++;
        else if(num==2)
            gasolina++;
        else if(num==3)
            dissel++;
        else
            continue;
    }
    cout<<"MUITO OBRIGADO"<<endl;
    cout<<"Alcool: "<<alcohol<<endl;
    cout<<"Gasolina: "<<gasolina<<endl;
    cout<<"Diesel: "<<dissel<<endl;
    return 0;
}
